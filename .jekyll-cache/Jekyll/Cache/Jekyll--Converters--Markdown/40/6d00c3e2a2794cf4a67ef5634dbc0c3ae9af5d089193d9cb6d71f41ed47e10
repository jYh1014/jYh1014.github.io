        this.set <span class="o">=</span> <span class="o">{}</span>
    <span class="o">}</span>
    add<span class="o">(</span>ele<span class="o">){</span>
        <span class="k">if</span><span class="o">(!</span>this.set.hasOwnProperty<span class="o">(</span>ele<span class="o">)){</span>
            this.set[ele] <span class="o">=</span> ele
        <span class="o">}</span>
    <span class="o">}</span>
   
<span class="o">}</span>
