</pre></td> --><td class="rouge-code"><pre>class Map<span class="o">{</span>
    constructor<span class="o">(){</span>
        this.arr <span class="o">=</span> <span class="o">{}</span>
    <span class="o">}</span>
    calc<span class="o">(</span>key<span class="o">)</span> <span class="o">{</span>
        <span class="nb">let </span>total <span class="o">=</span> 0
        <span class="k">for</span> <span class="o">(</span><span class="nb">let </span>i <span class="o">=</span> 0<span class="p">;</span> i &lt; key.length<span class="p">;</span> i++<span class="o">)</span> <span class="o">{</span>
            total +<span class="o">=</span> key[i].charCodeAt<span class="o">()</span>
        <span class="o">}</span>
        <span class="k">return </span>total % 100
    <span class="o">}</span>
    <span class="nb">set</span><span class="o">(</span>key, value<span class="o">)</span> <span class="o">{</span>
        key <span class="o">=</span> this.calc<span class="o">(</span>key<span class="o">)</span>
        this.arr[key] <span class="o">=</span> value
    <span class="o">}</span>
    get<span class="o">(</span>key<span class="o">)</span> <span class="o">{</span>
        key <span class="o">=</span> this.calc<span class="o">(</span>key<span class="o">)</span>
        <span class="k">return </span>this.arr[key]
    <span class="o">}</span>
<span class="o">}</span>
<span class="nb">let </span>map <span class="o">=</span> new Map<span class="o">()</span>
map.set<span class="o">(</span><span class="s2">"abc"</span>, 123<span class="o">)</span>
console.log<span class="o">(</span>map.arr<span class="o">)</span>
</pre></td></tr></tbody></table></code></pre></div></div>
